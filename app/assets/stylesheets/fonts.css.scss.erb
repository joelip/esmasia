@font-face {
	font-family: 'NovecentoBook';
	src: url('<%= asset_path("NovecentoBook.eot") %>');
	src: url('<%= asset_path("NovecentoBook.eot?") %>') format('embedded-opentype'),
	url('<%= asset_path("NovecentoBook.woff") %>') format('woff'), 
  url('<%= asset_path("NovecentoBook.ttf") %>') format('truetype');
}
@font-face {
	font-family: 'NovecentoDemiBold';
	src: url('<%= asset_path("NovecentoDemiBold.eot") %>');
	src: url('<%= asset_path("NovecentoDemiBold.eot?") %>') format('embedded-opentype'),
	url('<%= asset_path("NovecentoDemiBold.woff") %>') format('woff'), 
  url('<%= asset_path("NovecentoDemiBold.ttf") %>') format('truetype');
}